# Configurações do Sistema Guardião para Discloud
# Copie este arquivo para discloud.env e preencha com suas credenciais reais

# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=False
ALLOWED_HOSTS=your-domain.discloud.app

# Database PostgreSQL
DB_NAME=guardiao_db
DB_USER=guardiao_user
DB_PASSWORD=your-db-password-here
DB_HOST=postgresql.discloud.app
DB_PORT=5432

# Discord Bot
DISCORD_BOT_TOKEN=your-discord-bot-token-here
DISCORD_CLIENT_ID=your-discord-client-id-here
DISCORD_CLIENT_SECRET=your-discord-client-secret-here

# Site URL
SITE_URL=https://your-domain.discloud.app

# Bot API URL
BOT_API_URL=http://localhost:8081
